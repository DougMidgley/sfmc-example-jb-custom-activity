{
  "plugins": ["jsdoc"],
  "extends": [
    "plugin:jsdoc/recommended",
    "prettier",
    "plugin:unicorn/recommended",
    "eslint:recommended"
  ],
  "env": {
    "browser": true,
    "node": true,
    "es6": true,
    "mocha": true
  },
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": 2022
  },
  "globals": {
    "assert": "readonly"
  }
}
